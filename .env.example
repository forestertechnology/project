# QR Menu SaaS - Environment Configuration

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Application Environment
VITE_APP_ENV=development
VITE_APP_DEBUG=true

# Authentication
VITE_AUTH_PROVIDER=supabase
VITE_ENABLE_SOCIAL_LOGIN=true

# Feature Flags
VITE_FEATURE_MULTI_LANGUAGE=false
VITE_FEATURE_ANALYTICS=false
VITE_FEATURE_AI_RECOMMENDATIONS=false

# Performance and Monitoring
VITE_PERFORMANCE_TRACKING=true
VITE_ERROR_TRACKING_ENABLED=true

# External Service Integrations
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# Subscription and Billing
VITE_STRIPE_PUBLISHABLE_KEY=
VITE_STRIPE_SECRET_KEY=

# Optional Advanced Configurations
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=3600  # Cache time-to-live in seconds

# Logging
VITE_LOG_LEVEL=info

# Security
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW=60  # In seconds

# Deployment Specific
VITE_DEPLOYMENT_REGION=us-east-1
VITE_DEPLOYMENT_ENVIRONMENT=staging

# Development Tools
VITE_MOCK_API=false
VITE_USE_MOCK_DATA=false

# Experimental Features
VITE_EXPERIMENTAL_FEATURES=false

# Notes:
# 1. Never commit actual secrets to version control
# 2. Copy this file to .env and fill in your actual values
# 3. Add .env to .gitignore to prevent accidental commits
